    "build": "NODE_ENV=production webpack --debug --config webpack.production.config.js --progress --colors",
    "watch": "NODE_ENV=development node server.js",
    "test": "NODE_ENV=development jest",
    "lint": "eslint -c .eslintrc client/"
  },
